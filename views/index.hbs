<div class="container-fluid header background-image">
  <div class="row header-title">
    <h1>All the films and series you ever wanted to watch, tracked in one place</h1>
    <button
      type="button"
      class="btn btn-warning cta-btn"
      data-bs-toggle="modal"
      data-bs-target="#exampleModal"
    >Sign up for free</button>
  </div>
  <div></div>

</div>
{{#if data}}
  <div id="film-results-container">
    <ul id="film-results-ul">
      {{#each data}}
        <li><a href="/film-details/{{this.id}}"><img
  <div class="container-fluid navbar-correction">
    <div class="row align-items-center">
      {{#each data}}
        <div class="col-md-2 col-films col-sm-12 img-container">
          <a href="/film-details/{{this.id}}"><img
              class="image"
              src="{{this.first_url_string}}{{this.poster_path}}"
              alt="{{this.original_title}} poster"
            />
          </a>
          <br />
          <h6>{{this.original_title}}</h6>
        </li>
      {{/each}}
    </ul>
  </div>
          <div class="overlay">
            <div class="action-icon-films">
              <a href="/film-details/{{this.id}}"><i class="bi bi-bookmark-plus icon-films"></i></a>
              <a href="/film-details/{{this.id}}"><i class="bi bi-star-fill icon-films"></i></a>
              <a href="/film-details/{{this.id}}"><i class="bi bi-share-fill icon-films" alt="share"></i></a>
            </div>
            <a href="/film-details/{{this.id}}"><h5
              >{{this.original_title}}</h6></a>
          </div>
        </div>
      {{/each}}
    </div>
  </div>

{{/if}}